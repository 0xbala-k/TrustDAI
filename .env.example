# TrustDAI Environment Configuration
# Create two files from this example: .env and .env-ai

# Environment
VITE_APP_ENV=dev                         # Use 'dev' for main env and 'dev-ai' for AI env

# Blockchain Network
VITE_CHAIN_ID=11155111                   # 11155111 for Sepolia Testnet
VITE_CONTRACT_ADDRESS=0x123456789abcdef123456789abcdef12345678   # Your TrustDAI contract
VITE_RPC_URL=https://sepolia.infura.io/v3/your-infura-key   # Ethereum RPC provider

# Storage
VITE_ETHSTORAGE_ENDPOINT=https://eth.sep.w3link.io/   # EthStorage gateway endpoint
VITE_IPFS_GATEWAY=https://ipfs.io/ipfs/               # IPFS gateway for compatibility

# Web3URL Configuration
VITE_FLAT_DIRECTORY_ADDRESS=0x123456789abcdef123456789abcdef12345678  # Your FlatDirectory contract address
VITE_ENS_NAME=your-name.eth              # Optional: Your ENS name (if you have one)
VITE_WEB3URL_ENABLED=true                # Enable Web3URL format for links

# Lit Protocol Settings
VITE_LIT_PROTOCOL_ENABLED=true           # Enable Lit Protocol encryption
VITE_LIT_PROTOCOL_CHAIN_ID=175188        # Chronicle Yellowstone testnet
VITE_LIT_PROTOCOL_RPC_URL=https://yellowstone-rpc.litprotocol.com/

# QuarkChain Settings
VITE_QUARKCHAIN_RPC_URL=https://rpc.beta.testnet.l2.quarkchain.io:8545/
VITE_QUARKCHAIN_CHAIN_ID=3335

# UI Configuration
VITE_APP_NAME=TrustDAI
VITE_APP_DESCRIPTION=Decentralized Personal Data Sharing
VITE_DEFAULT_LOCALE=en-US 