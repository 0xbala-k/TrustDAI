# ===== ETHEREUM NETWORK CONFIGURATION =====
# Sepolia testnet is used for the TrustDAI contract
ETHEREUM_NETWORK=sepolia

# ===== TRUSTDAI CONTRACT CONFIGURATION =====
# The address of your deployed TrustDAI contract on Sepolia
CONTRACT_ADDRESS=0x123456789abcdef123456789abcdef123456789a

# ===== ETHSTORAGE CONFIGURATION =====
# RPC URL specifically for EthStorage (QuarkChain L2 RPC for general blockchain operations)
ETHSTORAGE_RPC_URL=https://rpc.beta.testnet.l2.quarkchain.io:8545/

# Blob Archiver API URL (EthStorage RPC for blob storage operations)
ETHSTORAGE_BLOB_ARCHIVER_API=https://rpc.beta.testnet.l2.ethstorage.io:9596/

# Wallet private key (without 0x prefix) - KEEP SECURE!
# This same key will be used for both Sepolia and EthStorage operations
PRIVATE_KEY=your_private_key_here

# EthStorage contract address for QuarkChain L2 TestNet (chainId 3335)
ETHSTORAGE_CONTRACT_ADDRESS=0x64003adbdf3014f7E38FC6BE752EB047b95da89A

# ===== API CONFIGURATION =====
# Optional API keys for enhanced service
ETHERSCAN_API_KEY=your_etherscan_api_key
INFURA_API_KEY=your_infura_api_key
ALCHEMY_API_KEY=your_alchemy_api_key_here

# ===== DEVELOPMENT SETTINGS =====
# Set to true for development/testing, false for production
DEBUG=true

# TrustDAI Environment Configuration Sample
# Copy this file to .env or .env-ai for production use

# Ethereum Network
# RPC URL for the Ethereum network (Sepolia testnet recommended for testing)
ETHEREUM_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# Contract address for TrustDAI
TRUSTDAI_CONTRACT_ADDRESS=0x123456789abcdef123456789abcdef123456789a

# Wallet Configuration
# Private key for transactions (KEEP THIS SECRET!)
# This should be different for .env and .env-ai to simulate different wallets
WALLET_PRIVATE_KEY=your_wallet_private_key_here

# Lit Protocol Configuration
# Lit Protocol network to use (serrano for testnet, habanero for mainnet)
LIT_PROTOCOL_NETWORK=serrano

# PKPS Auth (if using PKPs with Lit Protocol)
LIT_PKPS_PUBLIC_KEY=

# Authentication Signature (optional for testing)
LIT_AUTH_SIGNATURE=

# Lit Protocol Client Configuration
LIT_ENABLE_LOGGING=true
LIT_DEBUG_MODE=true
LIT_RETRY_DELAY_SECONDS=2
LIT_CONNECT_TIMEOUT_SECONDS=30
LIT_RPC_URL_OVERRIDE=

# Test Configuration
# Enable state-changing tests (set to true to allow tests that modify blockchain state)
ENABLE_STATE_CHANGING_TESTS=false

# Test wallet private key (can be different from main wallet for testing)
TEST_WALLET_PRIVATE_KEY=your_test_wallet_private_key_here
TEST_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# IPFS Configuration
IPFS_GATEWAY_URL=https://ipfs.io/ipfs/
IPFS_API_URL=https://api.pinata.cloud/pinning/
IPFS_API_KEY=your_pinata_api_key
IPFS_API_SECRET=your_pinata_secret

# ElizaOS Integration (for RAG)
ELIZAOS_API_URL=https://api.elizaos.com/v1
ELIZAOS_API_KEY=your_elizaos_api_key 